<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Binevenido
			{% endblock %}
		</title>
		<link
		rel="shortcut icon" type="image/x-icon" href="../assets/images/logo.png"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<link href="../../assets/css/global.css" rel="stylesheet" type="text/css">
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
			<link
			rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
			<!-- jQuery library -->
			<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
			<!-- Popper JS -->
			<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
			<!-- Latest compiled JavaScript -->
			<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
			<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
		{% endblock %}

		{% block javascripts %}{% endblock %}
	</head>
	<body>
		{% block barra %}
			<nav class="navbar navbar-expand-lg navbar-light" style="background-color:#16626f !important">
				<a class="navbar-brand" href="/dashboard_admin"><img src="../assets/images/logo2.png" class="img-thumbnail" width="100px" height="auto"></a>
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon" style="color:#ffffff"></span>
				</button>
				<div class="collapse navbar-collapse text-left" id="navbarSupportedContent">
					<ul class="nav navbar-nav navbar-left text-light">
						<li class="nav-item">
							<a class="nav-link text-light" href="/user/" title='Administración de Usuarios del Sistema'>Usuarios</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-light" href="/periodo/" title='Administración de Periodos de Carga de Datos del Sistema'>Periodos</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-light" href="/ente/" title='Administración de Organismos Acreditadores en el Sistema'>Acreditadores</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-light" href="/categoria/" title='Administración de Categorias de Indicadores en el Sistema'>Categorias</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-light" href="/indicador/" title='Administración de Indicadores en el Sistema'>Indicadores</a>
						</li>
					</ul>
					&nbsp;
					<ul class="nav navbar-nav navbar-left text-light">
						<li class="dropdown">
							<a href="/#" class="dropdown-toggle text-light" data-toggle="dropdown">Indicadores<b class="caret"></b>
							</a>
							<ul class="dropdown-menu">
								<li style='font-size:0.8em; padding:3px'>
									<a href="#" title='CNED'>CNED</a>
								</li>
								<li style='font-size:0.8em; padding:3px'>
									<a href="#" title='CNA'>CNA</a>
								</li>
							</ul>
						</li>
					</ul>
					&nbsp;
					<ul class="nav navbar-nav navbar-left text-light">
						<li class="dropdown">
							<a href="/#" class="dropdown-toggle text-light" data-toggle="dropdown">Reportes<b class="caret"></b>
							</a>
							<ul class="dropdown-menu">
								<li style='font-size:0.8em; padding:3px'>
									<a href="#" title='CNED'>CNED</a>
								</li>
								<li style='font-size:0.8em; padding:3px'>
									<a href="#" title='CNA'>CNA</a>
								</li>
							</ul>
						</li>
					</ul>
					&nbsp;
					<ul class="navbar-nav mr-auto">
						<ul class="navbar-nav mr-auto text-light">
							<li class="nav-item">
								<a class="nav-link text-light" href="../../../logout">Salir</a>
							</li>
						</ul>
					</ul>
				</div>
			</nav>
			<hr>
		{% endblock %}
		{% block body %}{% endblock %}
		{% block modales %}
			<div class="modal fade" id="myModal" role="dialog">
				<div class="modal-dialog modal-dialog-centered">
					<div class="modal-content">
						<div class="modal-header">
							<h5 id="title_modal" class="modal-title">Espere por favor</h5>
							<button type="button" class="close" data-dismiss="modal">&times;</button>
						</div>
						<div class="modal-body">
							<p id="content_modal" align="center"><img src="../../assets/images/wait2.gif" width="50" height="50"></p>
						</div>
					</div>
				</div>
			</div>
		{% endblock %}
	</body>
</html>
