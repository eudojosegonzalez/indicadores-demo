{% extends 'base_dashboard_admin2.html.twig' %}

{% block title %}Organismos Acreditadores
{% endblock %}

{% block body %}
	<h1>Organismos Acreditadores</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Nombre</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for ente in entes %}
				<tr>
					<td>{{ ente.id }}</td>
					<td>{{ ente.nombre }}</td>
					<td>
						<a href="{{ path('app_ente_show', {'id': ente.id}) }}">show</a>
						<a href="{{ path('app_ente_edit', {'id': ente.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_ente_new') }}">Create new</a>
{% endblock %}
